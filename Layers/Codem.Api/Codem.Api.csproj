<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Configurations>Release;Develop;Local</Configurations>
        <Platforms>AnyCPU</Platforms>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Develop' ">
      <DebugSymbols Condition=" '$(DebugSymbols)' == '' ">true</DebugSymbols>
      <Optimize Condition=" '$(Optimize)' == '' ">false</Optimize>
    </PropertyGroup>


    <ItemGroup>
      <ProjectReference Include="..\..\Libs\Сodem.Shared\Сodem.Shared.csproj" />
      <ProjectReference Include="..\Codem.Application\Codem.Application.csproj" />
      <ProjectReference Include="..\Codem.Infrastructure\Codem.Infrastructure.csproj" />
    </ItemGroup>



</Project>
